{
  "webroot": "wwwroot",
  "compilationOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },

  "dependencies": {
    "Microsoft.AspNetCore.Mvc.Razor": "1.0.0-rc2-20446",
    "Microsoft.AspNetCore.Mvc.TagHelpers": "1.0.0-rc2-20774",
    "Microsoft.AspNetCore.Hosting": "1.0.0-rc2-20446",
    "Microsoft.AspNetCore.WebUtilities": "1.0.0-rc2-20446",
    "Microsoft.AspNetCore.IISPlatformHandler": "1.0.0-rc2-20587",
    "ODataSample": "1.0.0-*"
  },

  "commands": {
    "web": "ODataSample.Web"
  },
  "tools": {
    "dotnet-publish-iis": "1.0.0-*"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": "4.0.10.0",
        "System.ComponentModel": "4.0.0.0",
        "System.Collections": "4.0.0.0",
        "System.Xml.ReaderWriter": "4.0.0.0",
        "System.IO": "4.0.0.0"
      },
      "dependencies": {
      }
    }
  },
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ],
  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ]
}
